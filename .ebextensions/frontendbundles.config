container_commands:
  Adownloadnode:
    command: "curl --silent --location https://rpm.nodesource.com/setup_5.x | bash -"
  Binstallnode:
    command: "yum -y install nodejs"
  Cnpminstall:
    command: "npm install"
  Dinstallbower:
    command: "npm install -g bower"
  Ebowerinstall:
    command: "bower install --allow-root"
  Fbundlecode:
    command: "npm build"
